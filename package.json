{
  "name": "IPL-Highlighting",
  "displayName": "ipl-highlighting",
  "description": "Interpreted Programming Language Highlighting for VSCode",
  "version": "1.0.0",
  "repository": {
  "type": "git",
  "url": "https://github.com/I-had-a-bad-idea/IPL-Highlighting.git"
  },
  "publisher": "I-had-a-bad-idea",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ipl",
      "aliases": ["IPL", "ipl"],
      "extensions": [".ipl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ipl",
      "scopeName": "source.ipl",
      "path": "./syntaxes/ipl.tmLanguage.json"
    }]
  }
}
